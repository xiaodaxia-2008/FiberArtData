
<?xml version="1.0" ?>
<robot name="DemoTrack">
    <!-- Properties -->
    <!-- link list -->
    <material name="gray">
        <color rgba="0.4 0.4 0.4 1" />
    </material>
    <material name="gray2">
        <color rgba="0.5 0.5 0.5 1" />
    </material>
    <link name="base_link">
        <visual>
            <geometry>
                <mesh filename="visual/base_link.stl" material_name="gray"/>
            </geometry>
        </visual>
    </link>
    <link name="link1">
        <visual>
            <geometry>
                <mesh filename="visual/link1.stl" material_name="gray2"/>
            </geometry>
        </visual>
    </link>
    <link name="ee_link"/>
    <!-- end of link list -->
    <!-- joint list -->
    <joint name="joint1" type="prismatic">
        <parent link="base_link"/>
        <child link="link1"/>
        <origin xyz="-0.065 0 0.375" rpy="0 0 0" />
        <axis xyz="0 1 0" />
        <limit lower="0" upper="15.0" velocity="1"/>
    </joint>
    <joint name="joint_1_t-tool0" type="fixed">
        <origin xyz="0.5 0.5 0.173" rpy="0 0 0"/>
        <parent link="link1"/>
        <child link="ee_link"/>
    </joint>
    <!-- end of joint list -->
</robot>
    